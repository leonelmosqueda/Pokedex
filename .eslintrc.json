{
    "env": {
        "browser": true,
        "es2021": true,
        "cypress/globals": true,
        "jest/globals": true,
        "node": true,
        "jest": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    },
    "plugins": [
        "cypress",
        "jest"
    ]
    
}
